{"ast":null,"code":"import { ref, watch, onMounted } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport CompanyOverview from \"@/components/CompanyOverview.vue\"; // Import the component\nimport StockIntradayChart from \"@/components/StockIntradayChart.vue\"; // Import the new IntradayChart component\n\nexport default {\n  components: {\n    CompanyOverview,\n    StockIntradayChart\n  },\n  setup() {\n    const route = useRoute();\n    const stockSymbol = ref(route.params.symbol);\n    console.log('Stock Symbol:', stockSymbol.value);\n    onMounted(fetchStockDetails);\n    watch(() => route.params.symbol, newSymbol => {\n      stockSymbol.value = newSymbol;\n      fetchStockDetails();\n    });\n    return {\n      stockSymbol\n    };\n  }\n};","map":{"version":3,"names":["ref","watch","onMounted","useRoute","CompanyOverview","StockIntradayChart","components","setup","route","stockSymbol","params","symbol","console","log","value","fetchStockDetails","newSymbol"],"sources":["/Users/attiqrehman/Documents/StockInfoSystem_FrontEnd/frontend/src/components/StockDetails.vue"],"sourcesContent":["<template>\n    <div class=\"container mt-4\">\n      <h2 class=\"text-center mb-4\">Stock Details: {{ stockSymbol }}</h2>\n  \n      <div v-if=\"stockData\">\n        <h3 class=\"text-center mt-4\">Company Overview</h3>\n      <CompanyOverview :symbol=\"stockSymbol\" />\n        <!-- Bar Chart for Market Cap -->\n        <h3 class=\"text-center mt-4\">Intraday Stock Price (60min)</h3>\n         <StockIntradayChart :symbol=\"stockSymbol\" />\n\n      </div>\n  \n      <div v-else class=\"text-center\">\n        <p>Loading stock details...</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import { ref, watch, onMounted } from \"vue\";\n  import { useRoute } from \"vue-router\";\n  import CompanyOverview from \"@/components/CompanyOverview.vue\"; // Import the component\n  import StockIntradayChart from \"@/components/StockIntradayChart.vue\"; // Import the new IntradayChart component\n\n  \n  export default {\n    components: { CompanyOverview, StockIntradayChart },\n    setup() {\n      const route = useRoute();\n      const stockSymbol = ref(route.params.symbol);\n      console.log('Stock Symbol:', stockSymbol.value);\n\n      \n  \n      onMounted(fetchStockDetails);\n  \n      watch(() => route.params.symbol, (newSymbol) => {\n        stockSymbol.value = newSymbol;\n        fetchStockDetails();\n      });\n  \n      return { stockSymbol };\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .container {\n    background: var(--bg-color);\n    color: var(--text-color);\n    padding: 20px;\n    border-radius: 10px;\n  }\n  \n  .chart-container {\n    width: 80%;\n    margin: auto;\n  }\n  </style>\n  "],"mappings":"AAoBE,SAASA,GAAG,EAAEC,KAAK,EAAEC,SAAQ,QAAS,KAAK;AAC3C,SAASC,QAAO,QAAS,YAAY;AACrC,OAAOC,eAAc,MAAO,kCAAkC,EAAE;AAChE,OAAOC,kBAAiB,MAAO,qCAAqC,EAAE;;AAGtE,eAAe;EACbC,UAAU,EAAE;IAAEF,eAAe;IAAEC;EAAmB,CAAC;EACnDE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC,CAAC;IACxB,MAAMM,WAAU,GAAIT,GAAG,CAACQ,KAAK,CAACE,MAAM,CAACC,MAAM,CAAC;IAC5CC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEJ,WAAW,CAACK,KAAK,CAAC;IAI/CZ,SAAS,CAACa,iBAAiB,CAAC;IAE5Bd,KAAK,CAAC,MAAMO,KAAK,CAACE,MAAM,CAACC,MAAM,EAAGK,SAAS,IAAK;MAC9CP,WAAW,CAACK,KAAI,GAAIE,SAAS;MAC7BD,iBAAiB,CAAC,CAAC;IACrB,CAAC,CAAC;IAEF,OAAO;MAAEN;IAAY,CAAC;EACxB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}